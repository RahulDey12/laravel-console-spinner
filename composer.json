{
    "name": "rahul900day/laravel-console-spinner",
    "description": "Laravel Console Spinner is a spinner output for Laravel command line.",
    "keywords": ["console", "command-line", "php", "cli", "laravel", "artisan", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rahul Dey",
            "email": "rahul900day@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Rahul900Day\\LaravelConsoleSpinner\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rahul900Day\\LaravelConsoleSpinner\\LaravelConsoleSpinnerServiceProvider"
            ]
        }
    }
}
